<!-- Modal -->
<div class="modal fade" id="updateJobModal" role="dialog">
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <!-- <h4 class="modal-title">Add Resume</h4> -->
      </div>

      <div class="modal-body">

        <form>

          <div class="form-group">
            <label> Job Title </label>
            <input type="title" name="title" class="form-control" id="title_edit" aria-describedby="emailHelp"
              placeholder="Job Trek Developer">
          </div>

          {{!-- <div class="form-group">
        <label for="companylist">Company</label>
        <select class="form-control" id="companylist">
            <option>Google</option>
            <option>JobTrek</option>
            <option>Trilogy</option>
            <option>Facebook</option>
            <option>Twitter</option>
        </select>
    </div> --}}

          <div class="form-group">
            <label> Location </label>
            <input type="location" name="location" class="form-control" id="location_edit" aria-describedby="emailHelp"
              placeholder="city, state">
          </div>

          <div class="form-group">
            <label> Job Description </label>
            <textarea class="form-control" id="jobdescription_edit" rows="3"></textarea>
          </div>

          <div class="form-group">
            <label> Job Requirement </label>
            <textarea class="form-control" name="jobrequirement" id="jobrequirement_edit" rows="3"> </textarea>
          </div>

          <div class="form-group">
            <label> Job Post </label>
            <input type="job-url" name="job-url" class="form-control" id="jobUrlEdit" value="{{dataValues.jobUrl}}"
              aria-describedby="emailHelp" placeholder="indeed.com">
          </div>

          <div class="form-group">
            <label> Notes </label>
            <textarea class="form-control" name="jobNotes" id="jobNotes_edit" rows="3">{{dataValues.note}} </textarea>
          </div>
          <div class="modal-footer">
            <button id="updatejobposting" type="submit" class="btn btn-primary"> Update Job
            </button>
        </form>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>


