          </div>

          <div class="row">

            <div style="border-radius: 4px; padding: 3px 3px 40px; overflow: auto; height: 100%;" class="list-group"
              id="col1" ondrop="dropIt(event)" ondragover="allowDrop(event)">
              {{#each wishlist}}
              <div draggable="true" ondragstart="dragStart(event)">
                <div style="padding: 2px 2px 10px;">
                  <div
                    style="background-color: rgba(144, 19, 254, 0.85); box-shadow: rgba(25, 4, 69, 0.15) 0px 0px 3px; border-radius: 4px; cursor: pointer; position: relative; border: 1px solid rgba(25, 4, 69, 0.1);">
                    <a id="infoLink" target="popup" href="/members/jobinfo"
                      onclick="window.open('/members/jobinfo', 'popup', 'width=600, height=600, right=50, top=50')"
                      class="list-group-item list-group-item-action flex-column align-items-start active"
                      style="display: flex; flex-direction: column; color: white; white-space: normal;"
                      data-id="{{dataValues.id}}" data-userid="{{dataValues.user_id}}">
                      <div class="d-flex w-100 justify-content-between">
                        <h5 id="jobtitle" class="mb-1">{{dataValues.job_title}}</h5>
                        <small>{{dataValues.createdAt}}</small>
                      </div>
                      <p id="job-url" class="mb-1">{{dataValues.url}}</p>

                      <div class="d-flex justify-content-between">
                        <button  data-delete="{{dataValues.id}}" class="btn deleteJob btn-danger">
                          <i class="fa fa-trash" style='font-size:14px' aria-hidden="true"></i>
                        </button>
                      </div>
                    </a>
                  </div>
                </div>
              </div>
              {{/each}}
