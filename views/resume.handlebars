    <div class="container">


        <div class="card">
            <div class="card-body">



                <div class="container">
                    <h2>Add New Resume</h2>
                    <!-- Trigger the modal with a button -->
                    <button type="button" class="btn btn-info" data-toggle="modal"
                        data-target="#myModal">ADD</button>

                    <!-- Modal -->
                    <div class="modal fade" id="myModal" role="dialog">
                        <div class="modal-dialog">

                            <!-- Modal content-->
                            <div class="modal-content">
                                <div class="modal-header">
                                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                                    <!-- <h4 class="modal-title">Add Resume</h4> -->
                                </div>

                                <div class="modal-body">
                                   
                                    <form>
                                        <div class="form-group">
                                            <label for="exampleFormControlFile1">Upload your resume</label>
                                            <input type="file" class="form-control-file" id="exampleFormControlFile1">
                                        </div>

                                        <label for="exampleFormControlFile1">Upload link to resume</label>
                                        <input type="text" class="form-control-file" id="resumeLink">
                                        <button type="submit" class="btn btn-primary">save</button>
                                    </form>

                                    <hr>
                                    <h3>create new resume</h3>
                                    <!-- complete new one -->
                                    <!-- new version out of an existing one -->

                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <hr>

                <nav>
                    <div class="nav nav-tabs" id="nav-tab" role="tablist">
                        <a class="nav-item nav-link active" id="stared-tab" data-toggle="tab" href="#stared-resume"
                            role="tab" aria-controls="stared-resume" aria-selected="true">Stared Resume</a>
                        <a class="nav-item nav-link" id="all-resume-tab" data-toggle="tab" href="#all-resume" role="tab"
                            aria-controls="all-resume" aria-selected="false">ALL Resumes</a>
                    </div>
                </nav>
                <br>
                <div class="tab-content" id="nav-tabContent">

                    <div id="stared-resume" class="card tab-pane fade show active" style="width: 18rem;">
                        {{#each resume}}
                    
                        <div class="card-body">
                            <h5 class="card-title">{{filename}}</h5>
                            <p class="card-text">
                            </p>

                        </div>
                        <div class="card-footer text-muted">
                           <p>{{role}}</p>
                            <a href="#" class="btn btn-primary" data-id = "{{id}}">open</a>
                        </div>
                        {{/each}}    

                    </div>

                    <div id="all-resume" class="card  tab-pane fade" style="width: 18rem;">

                        <div class="card-body">
                            <h5 class="card-title">two</h5>
                            <p class="card-text">With supporting text below as a natural lead-in to additional content.
                            </p>

                        </div>
                        <div class="card-footer text-muted">
                            Role
                            <a href="#" class="btn btn-primary">open</a>

                        </div>
                    </div>

                </div>

            </div>
        </div>

