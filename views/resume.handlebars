<nav class="navbar navbar-light bg-light">
    <a class="navbar-brand">Welcome, User! Manage your Resume here.</a>
    <form class="form-inline">

        <input class="form-control mr-sm-2" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-info" type="submit">Search</button>
        <button class="btn btn-light" type="submit">notifications</button>
    </form>
</nav>

<div class="container">


    <div class="card">
        <div class="card-body">

            {{> resumep/createResume}}

            <nav>
                <div class="nav nav-tabs" id="nav-tab" role="tablist">
                    <a class="nav-item nav-link active" id="stared-tab" data-toggle="tab" href="#stared-resume"
                        role="tab" aria-controls="stared-resume" aria-selected="true">Stared Resume</a>
                    <a class="nav-item nav-link" id="all-resume-tab" data-toggle="tab" href="#all-resume" role="tab"
                        aria-controls="all-resume" aria-selected="false">ALL Resumes</a>
                </div>
            </nav>

            <div class="tab-content" id="nav-tabContent">

                <div class="tab-pane fade show active" id="stared-resume" role="tabpanel"
                    aria-labelledby="nav-home-tab">
                    {{#each resume}}
                        {{#if dataValues.star}}
                            {{> resumep/starResume}}
                        {{/if}}
                    {{/each}}


                </div>

                <div class="tab-pane fade" id="all-resume" role="tabpanel" aria-labelledby="nav-profile-tab">
                    {{#each resume}}              
                        {{> resumep/starResume}}
                    {{/each}}
                </div>
            </div>

        </div>
    </div>

    <div class="card">
        <div class="card-body">
            were additional information goes
        </div>
    </div>
</div>



<script src="/js/resume.js"></script>